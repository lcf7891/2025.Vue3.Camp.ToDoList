{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "quoteProps": "preserve",
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "none",
  "endOfLine": "lf",
  "vueIndentScriptAndStyle": true,
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSpacing": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
